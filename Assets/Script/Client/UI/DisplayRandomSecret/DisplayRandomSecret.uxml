      <hr:UIButton name="_RandomSecretButton" class="Button"/>

      <hr:UILabel name="_RandomSecretLabel" class="RandomSecretLabel"  picking-mode="Ignore"></hr:UILabel>

    </hr:UILabel>
  </hr:UILuaView>

